/filanor/Desktop/js_learn/react/start-hooks/src/app/components/exercises/hocExercise.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/exercises/useRefExercise.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/exercises/childrenExercise.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/examples/children/reactChildrenExample.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/examples/useMemo/complexCalculate.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/examples/useRef/programmableActions.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/examples/memo/memoDependencyFunctionExample.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/examples/memo/memoWithUseCallbackExample.jsx",["207"],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/examples/cloneElement/cloneElementExample.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/examples/useRef/renderCount.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/examples/hoc/hocExample.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/examples/useRef/prevState.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/page/editUserPage/index.js",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/examples/hoc/someComponent.jsx",[],"/Users/filanor/Desktop/js_learn/react/start-hooks/src/app/components/page/editUserPage/editUserPage.jsx",["208","209"],"import React, { useEffect, useState } from \"react\";\nimport { useHistory, useParams } from \"react-router-dom\";\nimport { validator } from \"../../../utils/validator\";\nimport api from \"../../../api\";\nimport TextField from \"../../common/form/textField\";\nimport SelectField from \"../../common/form/selectField\";\n\nconst EditUserPage = () => {\n    const { userId } = useParams();\n    const history = useHistory();\n    const [isLoading, setIsLoading] = useState(false);\n    const [data, setData] = useState({\n        name: \"\",\n        email: \"\",\n        profession: \"\"\n    });\n    const [professions, setProfession] = useState([]);\n    const [qualities, setQualities] = useState({});\n    const [errors, setErrors] = useState({});\n    const getProfessionById = (id) => {\n        for (const prof in professions) {\n            const profData = professions[prof];\n            if (profData._id === id) return profData;\n        }\n    };\n    const getQualities = (elements) => {\n        const qualitiesArray = [];\n        for (const elem of elements) {\n            for (const quality in qualities) {\n                if (elem.value === qualities[quality]._id) {\n                    qualitiesArray.push(qualities[quality]);\n                }\n            }\n        }\n        return qualitiesArray;\n    };\n    const handleSubmit = (e) => {\n        e.preventDefault();\n        const isValid = validate();\n        if (!isValid) return;\n        const { profession, qualities } = data;\n        api.users\n            .update(userId, {\n                ...data,\n                profession: getProfessionById(profession),\n                qualities: getQualities(qualities)\n            })\n            .then((data) => history.push(`/users/${data._id}`));\n        console.log(data);\n    };\n    const transformData = (data) => {\n        return data.map((qual) => ({ label: qual.name, value: qual._id }));\n    };\n    useEffect(() => {\n        setIsLoading(true);\n        api.users.getById(userId).then(({ profession, qualities, ...data }) =>\n            setData((prevState) => ({\n                ...prevState,\n                ...data,\n                qualities: transformData(qualities),\n                profession: profession._id\n            }))\n        );\n        api.qualities.fetchAll().then((data) => setQualities(data));\n        api.professions.fetchAll().then((data) => setProfession(data));\n    }, []);\n    useEffect(() => {\n        if (data._id) setIsLoading(false);\n    }, [data]);\n\n    const validatorConfig = {\n        email: {\n            isRequired: {\n                message: \"Электронная почта обязательна для заполнения\"\n            },\n            isEmail: {\n                message: \"Email введен некорректно\"\n            }\n        },\n        name: {\n            isRequired: {\n                message: \"Введите ваше имя\"\n            }\n        }\n    };\n    useEffect(() => {\n        validate();\n    }, [data]);\n    const handleChange = (target) => {\n        setData((prevState) => ({\n            ...prevState,\n            [target.name]: target.value\n        }));\n    };\n    const validate = () => {\n        const errors = validator(data, validatorConfig);\n        setErrors(errors);\n        return Object.keys(errors).length === 0;\n    };\n    const isValid = Object.keys(errors).length === 0;\n    return (\n        <div className=\"container mt-5\">\n            <div className=\"row\">\n                <div className=\"col-md-6 offset-md-3 shadow p-4\">\n                    {!isLoading && Object.keys(professions).length > 0 ? (\n                        <form onSubmit={handleSubmit}>\n                            <TextField\n                                label=\"Имя\"\n                                name=\"name\"\n                                value={data.name}\n                                onChange={handleChange}\n                                error={errors.name}\n                            />\n                            <TextField\n                                label=\"Электронная почта\"\n                                name=\"email\"\n                                value={data.email}\n                                onChange={handleChange}\n                                error={errors.email}\n               